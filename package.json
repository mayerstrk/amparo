{
  "name": "amparo-fastify",
  "version": "0.1.8",
  "description": "meta framework for beautiful, more expressive, safer typescript",
  "author": "mstark",
  "license": "mit",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./common": {
      "import": "./dist/common/index.js",
      "require": "./dist/common/index.js",
      "types": "./dist/common/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./fastify": {
      "import": "./dist/fastify/index.js",
      "require": "./dist/fastify/index.js",
      "types": "./dist/fastify/index.d.ts"
    },
    "./pg": {
      "import": "./dist/pg/index.js",
      "require": "./dist/pg/index.js",
      "types": "./dist/pg/index.d.ts"
    },
    "./zod": {
      "import": "./dist/zod/index.js",
      "require": "./dist/zod/index.js",
      "types": "./dist/zod/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublishonly": "npm run build"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "http-status-codes": "^2.3.0"
  },
  "peerDependencies": {
    "fastify": "^5.2.1",
    "pg": "^8.0.0",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.7.9",
    "@types/pg": "^8.11.11",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  }
}
